<!DOCTYPE ui:UiBinder 
  SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent"
>
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
	xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form"
	xmlns:toolbar="urn:import:com.sencha.gxt.widget.core.client.toolbar"
	xmlns:menu="urn:import:com.sencha.gxt.widget.core.client.menu" xmlns:a="urn:import:com.google.gwt.dev.asm"
	xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button">
	<!-- xmlns:gxtcontainer="urn:import:com.sencha.gxt.widget.core.client.container" 
		xmlns:bld= "urn:import:com.sencha.gxt.widget.core.client.container" -->


	<ui:style>
	.panelCentered {
		margin-right: auto;
		margin-left: auto;
		border-top: 0px solid #666;
		border-right: 0px solid #666;
		border-left: 0px solid #999;
		vertical-align: middle;
		text-align: center;
		background-color: beige;
	}
	
	.statusDiv {
		text-align: right;
		margin: 1em;
	}
	
	.titleDiv {
		text-align: center;
		margin: 0em;
	}
	
	.linksDiv {
		text-align: right;
	}
	
	.centerMargin {
		margin: 10px;
	}
	
	.background {
		background-color: white;
	}
	</ui:style>

	<ui:with type="com.sencha.gxt.widget.core.client.container.MarginData"
		field="outerDataTP" />
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"
		field="northDataTP" />
	<ui:with type="com.sencha.gxt.widget.core.client.container.MarginData"
		field="centerDataTP" />
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"
		field="eastDataTP" />

	<ui:with type="com.sencha.gxt.core.client.util.Padding" field="paddingUserName">
		<ui:attributes top="0" right="10" bottom="0" left="2" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Padding" field="paddingComboboxes">
		<ui:attributes top="0" right="2" bottom="0" left="2" />
	</ui:with>


	<ui:with type="com.vw.ide.client.images.IdeImages" field="images" />

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData"
		field="toolBarRowData">
		<ui:attributes width="1" height="-1" />
	</ui:with>

	<!-- <container:SimpleContainer ui:field="outer" resize="true"> <container:BorderLayoutContainer 
		ui:field="con" borders="true"> -->
	<container:SimpleContainer height="49px"
		borders="false">
		<!-- <container:child layoutData="{outerDataTP}"> -->
		<container:BorderLayoutContainer
			ui:field="conTP" borders="false">

			<container:north layoutData="{northDataTP}">
				<menu:MenuBar borders="false">
					<menu:MenuBarItem text="File">
						<menu:menu>
							<menu:Menu>
								<menu:MenuItem text="New" icon="{images.new_con_en}" />
								<menu:MenuItem text="Open File">
								</menu:MenuItem>
								<menu:MenuItem text="Save" icon="{images.save_edit_dis}" />
								<menu:MenuItem text="Save all" icon="{images.saveall_edit_dis}" />
								<menu:MenuItem text="Close" icon="{images.close}" />
								<menu:SeparatorMenuItem />
								<menu:MenuItem text="Settings" />
							</menu:Menu>
						</menu:menu>
					</menu:MenuBarItem>
					<menu:MenuBarItem text="Project">
						<menu:menu>
							<menu:Menu>
								<menu:MenuItem text="New" icon="{images.new_wiz_en}"
									ui:field="newVwmlProjField" />
								<menu:MenuItem text="Open File" icon="{images.prj_obj}">
								</menu:MenuItem>
								<menu:MenuItem text="Save" />
								<menu:MenuItem text="Close" />
								<menu:MenuItem text="Add VWML files to project" />
								<menu:MenuItem text="Remove VWML files from project" />
								<menu:SeparatorMenuItem />
								<menu:MenuItem text="Settings" />
							</menu:Menu>
						</menu:menu>
					</menu:MenuBarItem>
					<menu:MenuBarItem text="Build">
						<menu:menu>
							<menu:Menu>
								<menu:MenuItem text="Compile VWML to Java" />
								<menu:MenuItem text="Run Java code" />
								<menu:MenuItem text="Compile and Run" />
								<menu:SeparatorMenuItem />
								<menu:MenuItem text="Settings" />
							</menu:Menu>
						</menu:menu>
					</menu:MenuBarItem>
					<menu:MenuBarItem text="View">
						<menu:menu>
							<menu:Menu>
								<menu:MenuItem text="Toggle standard fringes views">
									<menu:submenu>
										<menu:Menu ui:field="viewsMenu">
											<menu:CheckMenuItem text="Input" group="radios"
												checked="true" />
											<menu:CheckMenuItem text="Output" group="radios" />
											<menu:CheckMenuItem text="Debug" group="radios" />
										</menu:Menu>
									</menu:submenu>
								</menu:MenuItem>
							</menu:Menu>
						</menu:menu>
					</menu:MenuBarItem>
					<menu:MenuBarItem text="Fringes">
						<menu:menu>
							<menu:Menu ui:field="fringesMenu">
								<menu:MenuItem text="Install new" />
								<menu:MenuItem text="Remove" />
								<menu:MenuItem text="List of installed" />
							</menu:Menu>
						</menu:menu>
					</menu:MenuBarItem>
					<menu:MenuBarItem text="Help">
						<menu:menu>
							<menu:Menu>
								<menu:MenuItem text="Reference" />
								<menu:MenuItem text="About" ui:field="miHelpAboutField" />
							</menu:Menu>
						</menu:menu>
					</menu:MenuBarItem>
				</menu:MenuBar>
			</container:north>

			<container:center layoutData="{centerDataTP}">
				<toolbar:ToolBar borders="false">
					<button:TextButton icon="{images.new_con_en}"
						toolTip="New file" />
					<button:TextButton icon="{images.open_en}"
						toolTip="Open file" />
					<button:TextButton icon="{images.new_wiz_en}"
						itemId="idNewVwmlProjField" ui:field="bnNewVwmlProjField" toolTip="New project" />
					<toolbar:SeparatorToolItem />
					<button:TextButton icon="{images.save_edit_en}" ui:field="bnSaveFileField" 
								itemId="idSaveSelectedFile" toolTip="Save" />
					<button:TextButton icon="{images.saveall_edit_en}" ui:field="bnSaveAllField"
								itemId="idSaveAll" toolTip="Save All" />
					<toolbar:SeparatorToolItem />
					<button:TextButton icon="{images.exec_en}" ui:field="bnStartExecution"
						itemId="idStartExecution" toolTip="Compile and start execution" />
					<toolbar:SeparatorToolItem />
					<button:TextButton icon="{images.exec_en}" ui:field="bnContinueExecution"
						itemId="idContinueExecution" toolTip="Continue execution" />
					<button:TextButton icon="{images.suspend_en}" ui:field="bnSuspendExecution"
						itemId="idSuspendExecution" toolTip="Suspend" />
					<button:TextButton icon="{images.terminate_obj_en}" ui:field="bnTerminateExecution"
						itemId="idTerminateExecution" toolTip="Terminate" />
					<toolbar:SeparatorToolItem />
					<button:TextButton icon="{images.search_en}" ui:field="bnSearchText"
						itemId="idSearchText" toolTip="Search" />
					<toolbar:SeparatorToolItem />
					<toolbar:FillToolItem />
				</toolbar:ToolBar>
			</container:center>

			<container:east layoutData="{eastDataTP}">
				<toolbar:ToolBar borders="false">

					<button:TextButton text="Opened files">
						<button:menu>
							<menu:Menu ui:field="scrollMenu" maxHeight="500" minWidth="400"/>
						</button:menu>
					</button:TextButton>

					<toolbar:SeparatorToolItem />
					<!-- <container:HBoxLayoutContainer borders="false" hBoxLayoutAlign="MIDDLE" 
						padding="{paddingComboboxes}"> -->

<!-- 
					<button:TextButton text="Options" icon="{images.menu_show}">
						<button:menu>
							<menu:Menu ui:field="menu1">
								<menu:MenuItem text="m1" />
								<menu:MenuItem text="m2">
								</menu:MenuItem>
							</menu:Menu>
						</button:menu>
					</button:TextButton>
 -->




					<!-- </container:HBoxLayoutContainer> -->
<!-- 					<toolbar:SeparatorToolItem />
 -->
					<container:SimpleContainer ui:field="comboAceEditorPlaceCont">
					</container:SimpleContainer>
					<container:SimpleContainer ui:field="comboPlaceCont" visible="true">
					</container:SimpleContainer>



					<container:HBoxLayoutContainer width="150px"
						borders="false" hBoxLayoutAlign="MIDDLE" padding="{paddingUserName}">
						<container:SimpleContainer ui:field="userNamePlaceCont">
							<g:Label ui:field="userName" />
						</container:SimpleContainer>
						<toolbar:SeparatorToolItem />
						<button:TextButton text="logout" itemId="idUserLogout"
							ui:field="userLogout" />
					</container:HBoxLayoutContainer>

					<toolbar:FillToolItem />

				</toolbar:ToolBar>

			</container:east>
		</container:BorderLayoutContainer>
		<!-- </container:child> -->
	</container:SimpleContainer>

	<!-- </container:BorderLayoutContainer> </container:SimpleContainer> -->


</ui:UiBinder>
