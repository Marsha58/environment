<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"  xmlns:w="urn:import:com.google.gwt.widget.client">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.panelFramed {
			border-top: 1px solid #666;
			border-right: 1px solid #666;
			border-left: 1px solid #666;
			border-bottom: 1px solid #666;
			background-color: beige;
	  	}
 		.westPanel {
        	background-color: beige;
    	}
    	
	 	.centerPanel {
	        background-color: beige;
	    }
	</ui:style>
	<g:FlowPanel styleName="{style.panelFramed}" width="600px" height="700px">
		<g:CaptionPanel captionText="Fringe managment" width="96%" height="85%">
			<g:SplitLayoutPanel width="100%" height="95%">
				<g:west size="250" unit="PX">
					<g:FlowPanel styleName="{style.westPanel}" height="100%" width="100%">
						<g:Tree height="100%" width="100%">
						</g:Tree>
					</g:FlowPanel>
				</g:west>
				<g:center size="250" unit="PX">
					<g:FlowPanel styleName="{style.centerPanel}" height="100%" width="100%">
						<g:Tree height="100%" width="100%">
						</g:Tree>
					</g:FlowPanel>
				</g:center>
			</g:SplitLayoutPanel>
		</g:CaptionPanel>
		<g:LayoutPanel height="43px" width="100%">
			<g:layer left="81%" right="5px" top="3px" height="30px">
				<w:TextButton>Cancel</w:TextButton>
			</g:layer>
			<g:layer left="60%" right="115px" top="3px" height="30px">
				<w:TextButton>OK</w:TextButton>
			</g:layer>
		</g:LayoutPanel>
	</g:FlowPanel>
</ui:UiBinder> 