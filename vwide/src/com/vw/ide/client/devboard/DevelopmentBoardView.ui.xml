<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' 
	xmlns:vwp='urn:import:com.vw.ide.client.windowspanel' 
	xmlns:vwui='urn:import:com.vw.ide.client.ui'>

	<g:DockLayoutPanel unit='EM'>
		<g:north size='9'>
			<vwui:TopPanel ui:field='topPanel' />
		</g:north>

		<g:center>
			<g:SplitLayoutPanel>
				<g:west size='192'>
					<vwui:Shortcuts ui:field='shortcuts' />
				</g:west>

				<g:north size='600'>
					<vwui:EditorPanel ui:field='editorPanel' />
				</g:north>

				<g:center>
					<vwp:WindowsPanelView ui:field='windowsPanelView' />
				</g:center>
			</g:SplitLayoutPanel>
		</g:center>
	</g:DockLayoutPanel>

</ui:UiBinder>